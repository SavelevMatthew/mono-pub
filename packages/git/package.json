{
  "name": "@mono-pub/git",
  "version": "0.0.0-development",
  "description": "@mono-pub plugin to work with git repos",
  "repository": {
    "type": "git",
    "url": "git@github.com:SavelevMatthew/mono-pub.git",
    "directory": "packages/git"
  },
  "scripts": {
    "lint": "eslint src && prettier --check src",
    "build": "rollup -c"
  },
  "author": {
    "name": "Matthew Savelev",
    "email": "savelevmatthew@gmail.com",
    "url": "https://github.com/SavelevMatthew"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "./utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "import": "./dist/utils.mjs",
      "types": "./dist/utils.d.ts"
    }
  },
  "peerDependencies": {
    "mono-pub": "*"
  },
  "devDependencies": {
    "@mono-pub/configs": "workspace:^",
    "@rushstack/eslint-patch": "^1.2.0",
    "eslint": "^8.39.0",
    "mono-pub": "workspace:^",
    "rollup": "^3.21.5",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "execa": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
