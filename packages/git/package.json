{
  "name": "@mono-pub/git",
  "version": "0.0.0-development",
  "description": "@mono-pub plugin to work with git repos",
  "repository": {
    "type": "git",
    "url": "git@github.com:SavelevMatthew/mono-pub.git",
    "directory": "packages/git"
  },
  "scripts": {
    "lint": "eslint src && prettier --check src",
    "build": "tsup"
  },
  "author": {
    "name": "Matthew Savelev",
    "email": "savelevmatthew@gmail.com",
    "url": "https://github.com/SavelevMatthew"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "./utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "require": "./dist/utils/index.js",
      "import": "./dist/utils/index.mjs"
    }
  },
  "peerDependencies": {
    "mono-pub": "1.x"
  },
  "devDependencies": {
    "@mono-pub/configs": "workspace:^",
    "eslint": "^9.16.0",
    "mono-pub": "workspace:^",
    "tmp": "^0.2.1",
    "tsup": "^8.3.5",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "execa": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
